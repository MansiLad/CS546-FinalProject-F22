<title>{{title}}</title>
<main>
    <h1>{{title}}</h1>
    <form action="/propertyRegistration" method="POST">
        <label for="address">Apartment Number: </label>
        <input type="text" id="address" name="adress" placeholder="Enter Apartment Number"> <br>
        <label for="Street">Street: </label>
        <input type="text" id="Street" name="Street" placeholder="Enter the Street"> <br>
        <label for="City">City: </label>
        <input type="text" id="City" name="City" placeholder="Enter the City"> <br>
        <label for="State">State: </label>
        <input type="text" id="State" name="State" placeholder="Enter the State"> <br>
        <label for="zip">Zip Code: </label>
        <input type="text" id="zip" name="zip" placeholder="Enter the Zip Code"> <br>
        <label for="beds">Number of Beds: </label>
        <input type="text" id="beds" name="beds" placeholder="Enter the Beds"> <br>
        <label for="baths">Number of Baths </label>
        <input type="text" id="baths" name="baths" placeholder="Enter the Baths"> <br>
        <label for="Deposit">Deposit </label>
        <input type="text" id="Deposit" name="Deposit" placeholder="Enter the Deposit"> <br>
        <label for="Rent">Rent </label>
        <input type="text" id="Rent" name="Rent" placeholder="Enter the Rent"> <br>
        <label for="description">Decription of the Property </label>
        <input type="text" id="description" name="description" placeholder="Enter the description"> <br>
        <label for="amenities"> Amenities </label>
        <input type="text" id="amenities" name="amenities" placeholder="Enter the amenities"> <br>
        <input type="file" id="myFile" name="filename" placeholder="Upload images of the property" multiple> <br>
        <input type="submit" value="Submit">
        <button type="submit">Submit</button>
    </form>
</main>