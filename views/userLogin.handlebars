
    {{#if error}}
    <p>{{error}}</p>
    {{/if}}
    <div class="content">
        <form action="/userLogin" method="post">
            <div>
                <label for="emailInput" id="emailInputLabel">Email</label>
                <input type="email" name="emailInput" id="emailInput">
            </div><br>
            <div>
                <label for="passwordInput" id="passwordInputLabel">Password</label>
                <input type="password" name="passwordInput" id="passwordInput">
            </div>
            <div>
                <p class="error">{{error}}</p>
            </div>
            <div>
                <button type="submit" class="btn btn-default" id="login-button">Log In</button>
            </div>
        </form>
        <br>
        <br>

        <a href="/user/userRegistration">Coming for the first time? Register Here!</a>
    </div>

    
